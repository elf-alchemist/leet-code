{
  "name": "leet-code",
  "author": "Guilherme M. Miranda <alchemist.software@proton.me>",
  "license": "CC0-1.0",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "prepare": "git config --local core.hooksPath .githooks",
    "format": "prettier --config prettier.config.js --cache --ignore-unknown --write src/",
    "lint": "eslint --config eslint.config.js --cache --quiet --fix src/",
    "only": "_f() { node --test src/**/$1.js; }; _f",
    "all": "node --test src/**/**.js"
  },
  "devDependencies": {
    "@eslint/js": "^8.54.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/eslint": "^8.44.7",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.9.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "globals": "^13.23.0",
    "prettier": "^3.1.0"
  }
}
